address tso "ALTLIB ACT APPL(CLIST) DA('pref.SADBCLST')"
address ispexec "SELECT CMD(%ADBL PROD('pref') VIO(3390))",
                "SCRNAME(DB2ADM)"
address tso "ALTLIB DEACT APPL(EXEC)"
